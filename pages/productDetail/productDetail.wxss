/* 规格 */
.buy_product_detail {
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    width: 100%;
    height: auto;
    background: #FBFBFB;
    z-index: 50;
    background-color: #fff;
}
.buy_product_detail .product_detail .product_img {
    width: 150rpx;
    height: 150rpx;
    overflow: hidden;
    float: left;
    display: block;
}
.buy_product_detail .product_detail .product_text {
    float: left;
    margin-left: 20rpx;
    width: 75%;
    line-height: 36rpx;
    letter-spacing: 1px;
    font-size: 30rpx;
    padding: 20rpx 0;
}
.buy_product_detail .product_detail .product_text .price_name .price {
    font-size: 32rpx;
    color: #f51664;
}
.buy_product_detail .product_detail {
    border-bottom: 1px solid #f8f8f8;
