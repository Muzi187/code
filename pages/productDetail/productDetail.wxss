    margin: 20rpx 20rpx;
}
.buy_product_detail .spati {
     
    padding: 10rpx;
    margin: 20rpx 20rpx;
    overflow-y: auto;
    overflow-x: hidden;
}
.spati .specti_text .specti_textleft {
    float: left;
    width: 150rpx;
}
.spati .specti_text .specti_textright label {
    margin: 10rpx;
    padding:5rpx 10rpx;
    border-radius: 10rpx;
    border: 1px dotted #31bdff;
    color: #31bdff;
}
.spati .specti_text .specti_textright label radio{
  display: none;
}
.spati .specti_text .specti_textright {
    float: left;
    width: 75%;
}
.spati .specti_text .specti_textright .number{
  text-align: right;
}
.spati .specti_text .specti_textright label .active{
