.wap_slide{
  top: 0;
  background-color: rgba(0, 0, 0, 0.65);
  right: 0px;
  height: 100%;
  width: 100%;
  z-index: 9999;
  position: fixed;
  overflow: auto;
  left: 0px;
}
.wap_slide .detail_layer {
    padding: 0 20rpx;
    overflow-y: auto;
    background-color: #fff;
    position: fixed;
    width: 90%;
    height: 85%;
    margin: auto;
    left: 0;
    right: 0;
    bottom: 40rpx;
    border-radius: 40rpx;
    box-sizing: border-box;
}
.wap_slide .title {
  text-align: center;
  margin: 20rpx;
  line-height: 80rpx;
  font-size: 36rpx;
}
