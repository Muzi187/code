}
.huyuan_class {
  font-size: 24rpx;
  color: #f9fffb;
  margin-bottom: 10rpx;
}
.improvingData {
  width: 150rpx;
  bottom: 10rpx;
  background: #fff;
  position: absolute;
  right: -10rpx;
  border-radius: 30rpx;
