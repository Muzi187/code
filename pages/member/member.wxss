/* 头像 */

.top-numberDiv{
  padding-left: 20rpx;
  padding-top: 0;
  box-sizing: border-box;
  position: absolute;
  z-index: 100;
  top: 75rpx;
  left: 0px;
  right: 0px;
}
.top-number {
  padding-top: 70rpx;
  padding-left: 60rpx;
  box-sizing: border-box;
}
.top-pic-border {
    width: 130rpx;
    height: 130rpx;
    overflow: hidden;
    border-radius: 50%;
    background: #fff;
    box-sizing: border-box;
    border: 4px solid #ddeefb;
}
.img_show {
    width: 100%;
    height: 100%;
    overflow: hidden;
    margin: auto;
    border-radius: 50%;
}
.header_hy{
  text-align: center;
  color:#fff;
}
.number-infor {
    position: relative;
    width: 540rpx;
    box-sizing: border-box;
    padding: 0 20rpx;
}
.hy_name {
  color: #f9fffb;
  font-size: 34rpx;
  font-weight: 550;
  margin: 10rpx 0;
}
.progress {
    width: 300rpx;
    height: 10rpx;
    background: #fff;
    border-radius: 5rpx;
    margin: 20rpx 0;
    position: relative;
}
.progressLayer {
    max-width: 100%;
    height: 10rpx;
    background: #ffd802;
    border-radius: 5rpx;
    margin: 20rpx 0;
    min-width: 0%;
}
.progress .number {
  font-size: 24rpx;
  color: #fff;
  top: -12rpx;
  position: absolute;
  left: 320rpx;
}
.menberGrader {
  font-size: 24rpx;
