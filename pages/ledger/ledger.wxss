
/* list */
.chuli{
  border: 1px solid red;
  padding: 0 10rpx;
  border-radius: 6rpx;
}
.type_guz{
  border: 1px solid #1e91ec;
  padding: 0 15rpx;
  border-radius: 6rpx;
}
.sy_dindan_list .list {
  background-color: #fff;
  padding:10rpx 20rpx;
  margin: 10rpx 0;
}
.sy_dindan_list .list .shopName{
  font-size:28rpx; 
  padding: 10rpx;
}
.clearfix:after{
  display: block;
  content: '';
  overflow: hidden;
  clear: both;
}
.clearfix{
  zoom:1;
}
.order_list_div {
  border-bottom: 1px solid #f8f8f8;
}
.navigator-hover {
  background-color: #fff;
  float: right;
  font-size: 24rpx;

}
.dindan_hao .guige{
  color:#999;
  font-size: 24rpx;
}
.sy_dindan_list .list .shopName .fr{
  color:#ff6000;
}
.heji{
  color:#999;
  font-size: 24rpx;
  text-align: right;
}
.heji >text{
  color:#444;
  font-size: 24rpx;
  text-align: right;
}
.sy_dindan_list .list .dindan_pic {
  margin-top: 20rpx;
  width: 20%;
  height: 150rpx;
  text-align: center;
  border-radius: 10rpx;
  overflow: hidden;
  float: left;
}
.navigator-hover {
background-color: transparent;
opacity: 1;
}
.order_list_div {
    border-bottom: 1px solid #f8f8f8;
}
.sy_dindan_list .list .dindan_hao {
    width: 73%;
    padding: 20rpx;
    font-size: 24rpx;
    line-height: 40rpx;
}
.sy_dindan_list .list .order_name {
    width: 83%;
    color:#666;
}
.sy_dindan_list .list > view {
    line-height: 0.6rem;
    font-size: 0.3rem;
}
.element1 {
    word-break: break-all;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    overflow: hidden;
    text-overflow: ellipsis;
    -o-text-overflow: ellipsis;
    -webkit-text-overflow: ellipsis;
    -moz-text-overflow: ellipsis;
}

.sy_dindan_list .list .dindan_pic image{
  width:100%;
  height:100%;
}
.timeDate{
  color: #999;
  font-size: 24rpx;
  padding:20rpx;
}
.timeDate >text{
  margin-top: 10rpx;
}
.timeDate .sx{
  color: #a4a4a4;
  border: 1px solid #cfcdcd;
  padding: 10rpx 20rpx;
  border-radius: 5px;
  font-size: 26rpx;
  margin: 0 5rpx
}
.timeDate .sx:first-of-type{
  color: #ff7400;
  border: 1px solid #ff7400;
}
/* task list */
.task_list_layer{
  padding: 20rpx;
  background-color: #fff;
  margin-bottom: 10rpx;
}
.task_list_layer_title{
  padding: 0 20rpx;
  /* background-color: #fff; */
  border-bottom: 1px solid #f8f8f8;
}
.list_task{
  display: inline-block;
  width: 100%;
  background-color: #fff;
  box-sizing: border-box;
  position: relative;
  border-bottom: 1px solid #f5f5f5;
  border-radius: 10rpx;
  padding:10rpx 0;
}
.list_task:last-of-type{
  border-bottom: 1px solid #fff;
}
.fl_img{
  width: 160rpx;
  height: 160rpx;
  overflow: hidden;
  margin-bottom: 10rpx
}
.fr_text{
  padding: 10rpx;
}
.fr_text >.title{
  font-size: 30rpx;
  color: #666;
  margin-bottom: 10rpx;
}
.fr_text .title_ry{
  font-size: 28rpx;
  color: #666;
  margin-bottom: 10rpx;
  margin-top: 10rpx;
}
.time{
  font-size: 24rpx;
  color: #999;
}
.fr_text >.cl_ben{
  color:#1e91ec;
}
.fr_text > .time .zt_bj{
  width:36rpx;
  height:36rpx;
  margin-right:10rpx;
}
.fr_text > .type{
  font-size: 30rpx;
  color: #444;
  margin-bottom: 10rpx;
}
.list_task .state{
  position: absolute;
  right:20rpx;
  color: #74d3ff;
  border:1px solid #74d3ff;
  border-radius: 30rpx;
  padding:2px 10rpx;
  bottom: 20rpx;
  font-size:26rpx;
}
.tips_gj{
  font-size:26rpx;

}
