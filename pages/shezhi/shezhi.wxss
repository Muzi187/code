.list .left{
  width: 30%;
  text-align: left;
  color:#666;
}
.list .right{
  width: 53%;
  color: #666;
  padding: 0 20rpx;
  text-align: right;
  font-size: 26rpx;
}
.list {
  font-size: 30rpx;
  padding: 10rpx 0;
  border-bottom: 1px solid #fff3f7;
  line-height: 60rpx;
}
.tixian_layer{
  padding: 30rpx;
  background-color: #fff;
  margin-bottom: 10rpx;
}
.jiantou{
  margin-right:20rpx;
  margin-left:20rpx;
  color: #999;
}
.tijiao{
  background-color: #f51664;
  color:#fff;
  position: fixed;
  bottom: 0;
  width: 100%;
  left: 0;
  right:0;
  margin:auto;
}

/* 头像昵称 */
.pic_shezhi{
  width:80rpx;
  height:80rpx;
  border-radius: 50%;
  overflow: hidden;
  float:left;
}
.Nickname{
  font-size: 28rpx;
  color:#666;
  line-height: 40rpx;
  margin-left: 10rpx;
}
.number{
  font-size: 26rpx;
  color:#999;
  margin-left: 10rpx;
}
.edit{
  color: #ff6000;
  font-size: 26rpx;
  border: 1px solid #ff6000;
  padding: 0 20rpx;
  border-radius: 10rpx;
  height: 40rpx;
  line-height: 40rpx;
}
