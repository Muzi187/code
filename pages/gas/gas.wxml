<view class="header_top clearfix">
   <navigator hover-class="none" class="fl returnBotton" open-type="navigateBack" delta="1">
      <image src="../../image/icon/fll.png" class="fanhui_img"></image>
    </navigator>
    <view class="header_title fl text-center element1">
      打卡统计
    </view>
    <view class="empty|_top_right fl"></view>
</view>
<view class="empty_top"></view>

<view class="list_detail">
  <button class="zhuangtai url_botton"><text>月</text></button>
  <button class="url_botton">年</button>
</view>
<view class="section">
  <picker mode="date" value="{{date}}" start="2015-09-01" end="2017-09-01" bindchange="bindDateChange">
    <view class="picker">
      选择日期: {{date}}
    </view>
  </picker>
</view>

<view class="table">
  <view class="table-tr">
    <view class="table-th">打卡统计</view>
    <view class="table-th">次数</view>
  </view>
  <view class="table-tr">
    <view class="table-td">2020-02-02 10：00</view>
    <view class="table-td">1</view>
  </view>
  <view class="table-tr">
    <view class="table-td">2020-02-03 10：00</view>
    <view class="table-td">1</view>
  </view>
  <view class="table-tr">
    <view class="table-td">汇总</view>
    <view class="table-td">30</view>
  </view>
</view>

